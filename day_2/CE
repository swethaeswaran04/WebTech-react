import React from "react";
function Cw2()
{
    return(
        <div style={ {backgroundColor:'lightblue', color:'darkblue', padding:'10px',borderColor:'1px solid blue',border:'1px solid blue',borderRadius:'5px'}}>
            <h1 style={{color:"green"}}>Inline Style in JSX Example</h1>
            <p style={{
    color:'darkblue',
    fontSize:'16px'
}}>This is a paragraph with inline styles applied.</p>
        </div>
    )
}
